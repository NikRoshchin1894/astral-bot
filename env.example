TELEGRAM_BOT_TOKEN=your_bot_token_here
OPENAI_API_KEY=your_openai_api_key_here

# База данных (PostgreSQL на Railway/Render)
# Приоритет: сначала DATABASE_PUBLIC_URL, затем DATABASE_URL (для обратной совместимости)
DATABASE_PUBLIC_URL=postgresql://user:password@host:port/database
# DATABASE_URL=postgresql://user:password@host:port/database  # Альтернативный вариант

# Telegram Payment Provider Token (опционально)
# Получить можно через @BotFather командой /mybots -> Payments
# Если не указан, можно настроить платежного провайдера через BotFather
TELEGRAM_PAYMENT_PROVIDER_TOKEN=your_payment_provider_token_here
